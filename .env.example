# .env.example
# MySQL
MYSQL_ROOT_PASSWORD=rootpass
MYSQL_DATABASE=sailer
MYSQL_USER=sailer_user
MYSQL_PASSWORD=sailer_pass

# DB Connection
DB_DRIVER=mysql+pymysql
DB_USER=${MYSQL_USER}
DB_PASSWORD=${MYSQL_PASSWORD}
DB_HOST=db
DB_PORT=3306
DB_NAME=${MYSQL_DATABASE}

# Pool
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20
DB_POOL_RECYCLE=3600

# AI
AI_MODE=mock
SAILER_AI_API_KEY=
